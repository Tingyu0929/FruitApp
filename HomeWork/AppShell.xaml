<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="HomeWork.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:HomeWork"
    xmlns:week03="clr-namespace:HomeWork.Week03"
    xmlns:week04="clr-namespace:HomeWork.Week04"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    
    <ShellContent
        Title="Week03"
        ContentTemplate="{DataTemplate week03:Week3}"
        Route="Week03" />
    
    <ShellContent
        Title="Week04"
        ContentTemplate="{DataTemplate week04:Week04}"
        Route="Week04"/>
    
</Shell>
